{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adfCovidpp"
		},
		"ls_adls_covidreportadlspp_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls_covidreportadlspp'"
		},
		"ls_blob_covidreportsapp_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_blob_covidreportsapp'"
		},
		"ls_sqldb_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_sqldb'"
		},
		"ls_adls_covidreportadlspp_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://covidreportadlspp.dfs.core.windows.net"
		},
		"ls_http_github_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "@{linkedService().sourceBaseURL}"
		},
		"tr_population_event_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/5752d228-6856-4914-8673-1cae39b52f96/resourceGroups/covid_rg/providers/Microsoft.Storage/storageAccounts/covidreportsapp"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adfCovidpp_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_adls_covidreportadlspp_accountKey": {
						"value": "[parameters('ls_adls_covidreportadlspp_accountKey')]"
					},
					"ls_blob_covidreportsapp_connectionString": {
						"value": "[parameters('ls_blob_covidreportsapp_connectionString')]"
					},
					"ls_sqldb_connectionString": {
						"value": "[parameters('ls_sqldb_connectionString')]"
					},
					"ls_adls_covidreportadlspp_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_covidreportadlspp_properties_typeProperties_url')]"
					},
					"ls_http_github_properties_typeProperties_url": {
						"value": "[parameters('ls_http_github_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "adfCovidpp_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"tr_population_event_properties_typeProperties_scope": {
						"value": "[parameters('tr_population_event_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adfCovidpp_ArmTemplate_0')]"
			]
		}
	]
}